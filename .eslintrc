{
  "root": true,
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "printWidth": 100, "singleQuote": true, "trailingComma": "all", "endOfLine": "auto"}]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint"
}
